{
  "name": "eatwise-web",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:https": "concurrently --raw 'pnpm run dev' 'pnpm run ssl-proxy'",
    "ssl-proxy": "local-ssl-proxy --source 3001 --target 3000",
    "build": "next build",
    "start": "next start",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.mts --config eslint.config.js",
    "lint:stylelint": "stylelint '**/*.css' --config stylelint.config.js",
    "lint:tsc": "tsc --noEmit",
    "lint": "pnpm run lint:eslint && pnpm run lint:stylelint && pnpm run lint:tsc"
  },
  "engines": {
    "node": "^22.6.0"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.477.0",
    "next": "^15.2.0",
    "next-themes": "^0.4.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-spinners": "^0.15.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.17.5",
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@next/eslint-plugin-next": "*",
    "@tailwindcss/postcss": "^4.0.12",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.8",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "concurrently": "^9.1.2",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.2.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "*",
    "local-ssl-proxy": "^2.0.5",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-sort-imports": "^1.8.6",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "stylelint": "^16.15.0",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-prettier": "^5.0.3",
    "tailwindcss": "^4.0.12",
    "typescript": "~5.8.2",
    "typescript-eslint": "*"
  },
  "packageManager": "pnpm@10.5.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
